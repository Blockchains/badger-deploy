{
  "name": "rebase-contracts",
  "version": "0.0.1",
  "repository": "https://github.com/Rebase-Finance/rebase-contracts.git",
  "scripts": {
    "lint": "npx prettier ./deploy/*.ts --check",
    "lint:fix": "npx prettier ./deploy/*.ts --write",
