    "lodash": "^4.17.19",
    "openzeppelin-eth": "^2.0.2",
    "truffle-hdwallet-provider": "^1.0.17"
  },
  "devDependencies": {
    "@nomiclabs/buidler": "^1.4.3",
    "@nomiclabs/buidler-ethers": "^2.0.0",
    "@nomiclabs/buidler-etherscan": "^1.3.3",
    "@nomiclabs/buidler-waffle": "^2.0.0",
    "@types/mocha": "^8.0.1",
    "@types/node": "^14.0.27",
    "chai": "^4.2.0",
    "ethereum-waffle": "^3.0.2",
    "ethers": "^5.0.8",
    "ts-node": "^8.10.2",
